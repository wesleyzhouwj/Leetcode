class Solution {
    public int removeElement(int[] nums, int val) {
        if(nums.length == 0)return 0;
        
        int count = 0;
        for(int i=0;i<nums.length;i++){     //只需要把需要的替换到前面即可
            if(nums[i] != val){
                nums[count++] = nums[i];
            }
        }
        return count;
    }
}
